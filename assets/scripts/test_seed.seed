
class Program : APIDev::Object
{

  // Called when the object has been thrown.
  void
  on_thrown()
  {
    dev::log::info("Thrown");
  }


  // Called when the object has come in contact with another object.
  void
  on_contact()
  {
    dev::log::info("Contact");
  }


  //Called every now and then.
  void
  on_cycle()
  {
    dev::log::info("Thinking");
  }


  // Called when the object should be destroyed.
  void
  on_destroy()
  {
    dev::log::info("Destroy");
  }

};